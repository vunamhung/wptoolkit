{
	"name": "vunamhung/wptoolkit",
	"description": "This is a toolkit for WordPress developer.",
	"keywords": [
		"wordpress",
		"toolkit"
	],
	"homepage": "https://github.com/vunamhung/wptoolkit",
	"license": "GPL-3.0+",
	"support": {
		"issues": "https://github.com/vunamhung/wptoolkit/issues",
		"source": "https://github.com/vunamhung/wptoolkit"
	},
	"authors": [
		{
			"name": "Vu Nam Hung",
			"email": "vunamhung@gmail.com",
			"homepage": "https://github.com/vunamhung/",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": ">=5.6.0"
	},
	"autoload": {
		"files": [
			"bootstrap.php",
			"helpers/theme.php",
			"helpers/global.php",
			"helpers/core.php",
			"helpers/conditional.php",
			"helpers/general.php",
			"helpers/array.php",
			"helpers/woo.php",
			"helpers/http.php"
		],
		"psr-4": {
			"vnh\\": "."
		}
	},
	"archive": {
		"exclude": [
			"/bin",
			"/vendor",
			"/tests",
			"/scss",
			"/sass",
			"*/composer.*",
			"*/.*",
			"*.dist",
			"*.md",
			"*.scss",
			"LICENSE",
			"*.sh"
		]
	}
}
